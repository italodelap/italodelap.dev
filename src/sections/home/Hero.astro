---
import About from "./hero/About.astro";
import ExperienceYears from "./hero/ExperienceYears.astro";
import Location from "./hero/Location.astro";
import Photo from "./hero/Photo.astro";
import Profile from "./hero/Profile.astro";
---

<Photo class="row-span-2 col-span-12 order-2 xl:order-1 xl:col-span-4" />
<Profile
  class="row-span-2 col-span-12 order-1 xl:order-2 sm:col-span-6 xl:col-span-4"
/>
<About class="row-span-1 col-span-12 sm:col-span-6 xl:col-span-4 order-3" />
<ExperienceYears
  class="row-span-1 col-span-6 sm:col-span-3 lg:col-span-3 xl:col-span-2 order-4"
/>
<Location
  class="row-span-1 col-span-6 sm:col-span-3 lg:col-span-3 xl:col-span-2 order-5"
/>
